    "\n",
    "\n",
    "function vandermondematrix(t,n)\n",
    "    l = length(t)\n",
    "    V = zeros(l,n)\n",
    "    for i in 1:l\n",
    "        for j in 1:n\n",
    "            V[i,j] = t[i]^(j-1)\n",
    "        end\n",
    "    end\n",
    "    return V\n",
    "end\n"
   ]

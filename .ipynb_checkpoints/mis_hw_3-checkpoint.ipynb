    "\n",
    "\n",
    "function vandermondematrix(t,n)\n",
    "    m = length(t)\n",
    "    V = zeros(m,n)\n",
    "    for i in t\n",
    "        for j=1:n\n",
    "            V[i,j] = i^(j-1)\n",
    "        end\n",
    "    end\n",
    "    return V\n",
    "end\n",
    "\n",
    "\n",
    "t = [1,2,3]\n",
    "n = 5\n",
    "vandermondematrix(t,n)"
   ]
